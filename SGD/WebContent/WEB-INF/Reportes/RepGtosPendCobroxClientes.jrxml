	<queryString>
		<![CDATA[
SELECT
nro_docum
, cod_proceso
, carpeta
, fecha
,referencia
,moneda
,imp_tot_mo
,saldo FROM (
SELECT
g.nro_docum
, g.cod_proceso
, c.carpeta
, DATE_FORMAT(g.fecdoc, '%d-%m-%Y') fecha
,g.referencia
,(SELECT simbolo FROM m_monedas m WHERE m.cod_moneda = g.cod_moneda AND m.cod_emp = g.cod_emp) moneda
,g.imp_tot_mo
,(SELECT imp_tot_mo FROM sa_docum sa WHERE  sa.cod_docum = g.cod_docum AND sa.serie_docum = g.serie_docum AND sa.nro_docum = g.nro_docum AND sa.cod_emp = g.cod_emp) saldo
FROM c_gastos g,  c_procesos c
WHERE g.cod_proceso = c.cod_proceso AND g.cod_emp = c.cod_emp
AND g.cod_tit = $P{codTit} AND g.cod_emp = $P{codEmp} AND g.fecDoc <= $P{fecHasta} AND ( g.cod_moneda = $P{codMoneda} OR $P{codMoneda} = 'TODAS' ) ) AUX WHERE saldo > 0 ORDER BY saldo DESC
]]>
	</queryString>
